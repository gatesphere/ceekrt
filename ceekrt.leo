<t tx="peckj.20130228101737.1617">@first &lt;!doctype html&gt;
@language xml
@tabwidth -2

&lt;html&gt;
&lt;head&gt;
  &lt;&lt; meta &gt;&gt;
  &lt;&lt; style &gt;&gt;
  &lt;&lt; js &gt;&gt;
  &lt;&lt; title &gt;&gt;  
&lt;/head&gt;

&lt;body&gt;
  &lt;div class="container"&gt;
    &lt;&lt; header &gt;&gt;
    &lt;&lt; flash &gt;&gt;
    &lt;&lt; content &gt;&gt;
    &lt;&lt; footer &gt;&gt;
  &lt;/div&gt;
&lt;/body&gt;

&lt;/html&gt;

</t>
<t tx="peckj.20130228101737.1653">@language xml
@tabwidth -2

{% extends "base.html" %}

{% block title %}
&lt;title&gt;error {{ error }} | suspended-chord.info&lt;/title&gt;
{% endblock title %}

{% block content %}
  &lt;h1&gt;error {{ error }}&lt;/h1&gt;
  &lt;p&gt;Sorry.  Would you like to go back &lt;a href="{{ url_for('index') }}"&gt;home&lt;/a&gt;?&lt;/p&gt;
{% endblock content %}
</t>
<t tx="peckj.20130228101737.1680">@language xml
@tabwidth -2

{% extends "base.html" %}

{% block title %}
  &lt;title&gt;{{ page.title }} | suspended-chord.info&lt;/title&gt;
{% endblock title %}

{% block content %}
&lt;h1&gt;{{ page.title }}&lt;/h1&gt;
{# keep text legible - enforce short columns #}
&lt;div class="column"&gt;
  {{ page.html|safe }}
&lt;/div&gt;
{% endblock content %}

</t>
<t tx="peckj.20130228101737.1685">@language xml
@tabwidth -2

{% extends "base.html" %}

{% block content %}
&lt;h1&gt;placeholder&lt;/h1&gt;
&lt;p&gt;This page hasn't been created yet.&lt;/p&gt;
{% endblock content %}
</t>
