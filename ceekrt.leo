<t tx="peckj.20130228101737.1701">{% block flash %}
  {% with messages = get_flashed_messages() %}
    {% if messages %}
      {% for category, message in messages %}
        &lt;div id="{{ category }}"&gt;{{ message }}&lt;/div&gt;
      {% endfor %}
    {% endif %}
  {% endwith %}
{% endblock flash %}</t>
